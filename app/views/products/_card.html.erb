<div class="review">
  <div class="review-title">
    <%= "#{review.user.first_name} #{review.user.last_name} (#{review.user.email})" %>
  </div>
  <div class="review-body">
    <%= review.description %>
  </div>
  <div class="review-footer">
    <%=  "Rating: #{review.rating}/5" %>
  </div>
</div>
